---
pagination:
  data: themes
  size: 1
  alias: theme
permalink: "theme/{{ theme.slug }}/index.html"
---

{% extends "layouts/base.njk" %}

{% set title = theme.title.rendered %}

{% block pageTitle %}
	<h1 class="entry__title">{{ title | safe }}</h1>
{% endblock %}

{% block content %}
	{{ theme.content.rendered | safe }}
{% endblock %}
